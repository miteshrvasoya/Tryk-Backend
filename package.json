{
  "name": "tryk-backend",
  "version": "1.0.0",
  "description": "Backend for Tryk AI Support Bot",
  "main": "dist/app.js",
  "scripts": {
    "start": "npm run migrate:up && node dist/app.js",
    "dev": "ts-node-dev --respawn --transpile-only src/app.ts",
