    "axios": "^1.13.2",
    "bcryptjs": "^3.0.3",
    "bullmq": "^5.34.4",
    "cheerio": "^1.1.2",
    "cookie-parser": "^1.4.7",
    "cors": "^2.8.5",
    "dotenv": "^16.4.7",
    "express": "^4.21.2",
    "groq-sdk": "^0.37.0",
    "helmet": "^8.0.0",
